<%- include('header') %>
    <!-- sidebar-wrapper  -->
    <main class="page-content">
        <div class="container-fluid">
            <div class="container">
        

      
                <!-- Titre -->
                <div class="row d-flex justify-content-center text-center">
                    <div class="col-xl-12 col-md-12 mt-3">
                        <div class="card">
                            <div class="card-body">
                                <h1 class="">Ajouter un sauveuteur</h1>
                            </div>
                        </div>
                    </div>
                </div>
        
                <div class="row mt-3">
                    <form action="/ajouter_bateau"  method="POST" class="container" id="needs-validation" name="form_ordonnance" novalidate>
                        <!-- Client -->
                        <div class="row">
                            <div class="col-lg-12 col-sm-12 col-12">
                                <div class="form-group">
                                    <label class="text-inverse" for="inputNom">Nom</label>
                                    <input type="text" class="form-control" id="inputNom" name="inputNom" placeholder="Nom" required>
                                    <div class="invalid-feedback">
                                        Veuillez fournir un nom valide.
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-lg-12 col-sm-12 col-12">
                                <div class="form-group">
                                    <label class="text-inverse" for="inputNom">Pr√©nom</label>
                                    <input type="text" class="form-control" id="inputPrenom" name="inputPrenom" placeholder="Prenom" required>
                                    <div class="invalid-feedback">
                                        Veuillez fournir un prenom valide.
                                    </div>
                                </div>
                            </div>
                        </div>
        
                        <!-- Validation du formulaire -->
                        <hr>
                        <div class="row">
                            <div class="col-lg-12 col-sm-12 col-12 text-center">
                                <button class="btn btn-primary" style="width: 100%;" type="submit">Valider le
                                    formulaire</button>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
